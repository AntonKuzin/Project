@model IEnumerable<test.ViewModels.PictureViewModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Index";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>

            <div class ="pictures">
                @foreach (var item in Model) {
                    <div class="picture">
                        
                        @Html.Partial("_Picture", item)
                        
                    </div>
                }
            </div>
            
    

</body>
</html>
